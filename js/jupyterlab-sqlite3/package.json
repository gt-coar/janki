{
  "name": "@gt-coar/jupyterlab-sqlite3",
  "version": "0.1.0",
  "description": "basic model for working with sqlite3 files in JupyterLab",
  "dependencies": {
    "@jupyterlab/application": "3",
    "@jupyterlab/docmanager": "3",
    "@jupyterlab/fileeditor": "3",
    "@lumino/datagrid": "^0.25.0",
    "sql.js": "~1.5.0"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "georgia-tech",
    "sqlite"
  ],
  "homepage": "https://github.com/gt-coar/janki",
  "bugs": {
    "url": "https://github.com/gt-coar/janki/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "University System of Georgia and janki contributors",
    "email": "coar@gatech.edu"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.8",
    "@types/sql.js": "^1.4.2",
    "comlink": "^4.3.1",
    "comlink-loader": "^2.0.0",
    "react": "^17.0.1",
    "typescript": "~4.2.4"
  },
  "files": [
    "{lib,style,src}/**/*.{.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf,css}",
    "LICENSE.txt"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/gt-coar/janki"
  },
  "scripts": {
    "build:ext": "jupyter labextension build --debug .",
    "watch": "jupyter labextension watch --debug ."
  },
  "sideEffects": true,
  "jupyterlab": {
    "webpackConfig": "./webpack.config.js",
    "extension": "./lib/plugins/mimeHelper.js",
    "mimeExtension": "./lib/plugins/mime.js",
    "discovery": {
      "server": {
        "base": {
          "name": "jupyterlab-sqlite3"
        },
        "managers": [
          "pip"
        ]
      }
    },
    "outputDir": "../../py/jupyterlab-sqlite3/src/jupyterlab_sqlite3/labextensions/@gt-coar/jupyterlab-sqlite3",
    "sharedPackages": {
      "@lumino/datagrid": {
        "bundled": true,
        "singleton": false
      },
      "sql.js": {
        "bundled": true,
        "singleton": false
      }
    }
  },
  "styleModule": "style/index.js"
}
